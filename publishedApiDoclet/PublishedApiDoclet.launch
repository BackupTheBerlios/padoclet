<?xml version="1.0" encoding="UTF-8"?>
<launchConfiguration type="org.eclipse.jdt.launching.localJavaApplication">
<stringAttribute key="org.eclipse.jdt.launching.MAIN_TYPE" value="de.kruis.padoclet.PublishedApiDoclet"/>
<booleanAttribute key="org.eclipse.jdt.debug.ui.INCLUDE_EXTERNAL_JARS" value="true"/>
<stringAttribute key="org.eclipse.jdt.launching.PROGRAM_ARGUMENTS" value="-doclet de.kruis.padoclet.PublishedApiDoclet -sourcepath src -docletpath bin -source 1.4 -public -classpath bin;${system:JAVA_TOOLS} -subpackages de.kruis.padoclet -padFilterDefault &quot;sample\d*&quot; -padExcludeTag &quot;pad.exclude,undocumented&quot;"/>
<stringAttribute key="org.eclipse.jdt.launching.PROJECT_ATTR" value="publishedApiDoclet"/>
<stringAttribute key="org.eclipse.jdt.launching.VM_ARGUMENTS" value="-DPublishedApiDoclet.delegate=de.kruis.padoclet.RefCheckDoclet"/>
<booleanAttribute key="org.eclipse.debug.core.appendEnvironmentVariables" value="true"/>
<booleanAttribute key="org.eclipse.jdt.launching.STOP_IN_MAIN" value="true"/>
</launchConfiguration>
